    PATH_SUFFIXES ${MBASE_STD_INCLUDE_SUFFIX}
)

find_library(
    MBASE_STD_LIBRARIES
    PATH_SUFFIXES ${MBASE_STD_LIBRARY_SUFFIX}
)

if(MBASE_STD_INCLUDE_DIRS)
    set(MBASE_STD_FOUND ON)
