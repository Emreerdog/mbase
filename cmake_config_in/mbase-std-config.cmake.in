include_guard(GLOBAL)

set(MBASE_STD_LIB_NAME @MBASE_STD_LIB_NAME_EXCEPTION@)
set(MBASE_STD_INCLUDE_SUFFIX mbase-@MBASE_STD_LIB_NAME_EXCEPTION@)
set(MBASE_STD_LIBRARY_SUFFIX mbase-@MBASE_STD_LIB_NAME_EXCEPTION@)
set(MBASE_STD_VERSION @MBASE_STD_VERSION@)
set(MBASE_STD_NAME_DESCRIPTIVE "MBASE ${MBASE_STD_LIB_NAME} library ${MBASE_STD_VERSION}")

find_path(
    MBASE_STD_INCLUDE_DIRS
